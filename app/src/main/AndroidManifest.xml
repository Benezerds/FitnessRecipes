<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rezappstudio.fitnessrecipes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/chad"
        android:label="@string/app_name"
        android:roundIcon="@drawable/chad"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitnessRecipes">
        <activity
            android:name=".CuttingActivity"
            android:exported="false"
            android:label="Cutting Recipes"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".BulkingActivity"
            android:exported="false"
            android:label="Bulking Recipes"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>